[binaries]
c = 'mips-linux-gnu-gcc'
cpp = 'mips-linux-gnu-cpp'
ar = 'mips-linux-gnu-ar'
strip = 'mips-linux-gnu-strip'
pkgconfig = 'pkg-config'
exe_wrapper = ['qemu-mips', '-L', '/usr/mips-linux-gnu/', '-cpu', '74Kf']

[host_machine]
system = 'linux'
cpu_family = 'mips32'
cpu = 'mips32'
endian = 'big'
