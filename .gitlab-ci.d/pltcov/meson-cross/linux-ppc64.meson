[binaries]
c = 'powerpc64-linux-gnu-gcc'
cpp = 'powerpc64-linux-gnu-cpp'
ar = 'powerpc64-linux-gnu-ar'
strip = 'powerpc64-linux-gnu-strip'
pkgconfig = 'pkg-config'
exe_wrapper = ['qemu-ppc64', '-L', '/usr/powerpc64-linux-gnu/']

[host_machine]
system = 'linux'
cpu_family = 'ppc64'
cpu = 'ppc64'
endian = 'big'
